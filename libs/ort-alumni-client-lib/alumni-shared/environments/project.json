{
  "projectType": "library",
  "root": "libs/ort-alumni-client-lib/alumni-shared/environments",
  "sourceRoot": "libs/ort-alumni-client-lib/alumni-shared/environments/src",
  "prefix": "ort-alumni-web",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/ort-alumni-client-lib/alumni-shared/environments"
      ],
      "options": {
        "jestConfig": "libs/ort-alumni-client-lib/alumni-shared/environments/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/ort-alumni-client-lib/alumni-shared/environments/src/**/*.ts",
          "libs/ort-alumni-client-lib/alumni-shared/environments/src/**/*.html"
        ]
      }
    }
  },
  "tags": ["scope:client-lib", "type:shared-environments"]
}
